      "id": "policy_001",
      "title": "Benefits Policy",
      "type": "Benefits",
      "content": "The company provides comprehensive benefits including health insurance, dental, vision, 401(k) with company matching, life insurance, and disability coverage. Benefits enrollment occurs during the annual open enrollment period or within 30 days of hire. Contact HR for detailed benefit information.",
      "department": "All",
      "effective_date": "2025-04-20T11:20:04.296328",
      "last_updated": "2025-07-09T11:20:04.296332",
      "version": "v1.0",
      "approval_status": "Approved",
      "priority": "Medium",
      "doc_type": "Policy",
      "tags": [
        "benefits",
        "hr",
        "policy"
      ],
      "applies_to": "Full-time Only"
    },
    {
      "id": "policy_002",
      "title": "Safety Policy",
      "type": "Safety",
      "content": "Employee safety is our top priority. All employees must follow safety protocols, report hazards immediately, and participate in required safety training. Personal protective equipment will be provided where necessary. Emergency procedures are posted throughout the facility.",
      "department": "Marketing",
      "effective_date": "2025-03-01T11:20:04.296341",
      "last_updated": "2025-07-07T11:20:04.296344",
      "version": "v4.1",
      "approval_status": "Approved",
      "priority": "Low",
      "doc_type": "Policy",
      "tags": [
        "safety",
        "hr",
        "policy"
      ],
      "applies_to": "Managers Only"
    },
    {
      "id": "policy_003",
      "title": "Vacation Policy",
      "type": "Vacation",
      "content": "Employees are entitled to paid vacation time based on their length of service. New employees receive 10 days annually, increasing to 15 days after 2 years and 20 days after 5 years. Vacation requests must be submitted at least 2 weeks in advance and approved by the direct manager.",
      "department": "Marketing",
      "effective_date": "2025-06-29T11:20:04.296351",
