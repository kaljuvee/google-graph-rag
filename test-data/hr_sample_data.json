    },
    {
      "id": "policy_003",
      "title": "Vacation Policy",
      "type": "Vacation",
      "content": "Employees are entitled to paid vacation time based on their length of service. New employees receive 10 days annually, increasing to 15 days after 2 years and 20 days after 5 years. Vacation requests must be submitted at least 2 weeks in advance and approved by the direct manager.",
      "department": "HR",
      "effective_date": "2024-10-02T11:24:41.754832",
      "last_updated": "2025-07-22T11:24:41.754835",
      "version": "v1.4",
      "approval_status": "Approved",
