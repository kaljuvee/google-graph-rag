    {
      "id": "policy_006",
      "title": "Training Policy",
      "type": "Training",
      "content": "The company is committed to employee development through various training programs. All employees must complete mandatory compliance training annually. Professional development opportunities are available with manager approval. Training records are maintained by HR.",
      "department": "Engineering",
      "effective_date": "2025-03-14T11:20:04.296381",
