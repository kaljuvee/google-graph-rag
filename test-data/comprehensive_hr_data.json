      ],
      "applies_to": "Managers Only"
    },
    {
      "id": "policy_003",
      "title": "Benefits Policy",
      "type": "Benefits",
      "content": "The company provides comprehensive benefits including health insurance, dental, vision, 401(k) with company matching, life insurance, and disability coverage. Benefits enrollment occurs during the annual open enrollment period or within 30 days of hire. Contact HR for detailed benefit information.",
      "department": "Legal",
      "effective_date": "2024-11-18T11:24:03.408112",
