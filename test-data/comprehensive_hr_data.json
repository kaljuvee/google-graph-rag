    {
      "id": "policy_002",
      "title": "Sick Leave Policy",
      "type": "Sick Leave",
      "content": "All employees are provided with sick leave benefits. Full-time employees accrue 8 hours of sick leave per month. Sick leave can be used for personal illness, medical appointments, or caring for immediate family members. A doctor's note may be required for absences exceeding 3 consecutive days.",
      "department": "Finance",
      "effective_date": "2024-10-17T11:24:03.408102",
      "last_updated": "2025-07-14T11:24:03.408105",
